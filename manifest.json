{
    "manifest_version": 3,
    "name": "Coding Train Pop Up",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": ["tabs"],
    "action": {
      "default_icon": "icon-128x128.png",
      "default_popup": "popup.html",
      "default_title": "Your enhanced video!!"
    }
  }
  